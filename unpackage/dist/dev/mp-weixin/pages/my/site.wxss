.box {
  margin: 0rpx 48rpx;
  width: 654rpx;
  height: 102rpx;
  border-bottom: 2rpx solid #D8D8D8;
  overflow: hidden;
}
.box_l {
  float: left;
  margin-top: 48rpx;
  width: 140rpx;
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #333333;
  line-height: 40rpx;
}
.box_input {
  float: left;
  margin-top: 48rpx;
  margin-left: 20rpx;
  width: 300rpx;
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #333333;
  line-height: 40rpx;
}
.box_r {
  float: right;
  margin-top: 48rpx;
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #ADADAD;
  line-height: 40rpx;
}
.site {
  margin: 0rpx 48rpx;
  width: 654rpx;
  height: 156rpx;
  border-bottom: 2rpx solid #D8D8D8;
  overflow: hidden;
}
.site_l {
  float: left;
  margin-top: 48rpx;
  width: 140rpx;
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #333333;
  line-height: 40rpx;
}
.site_textarea {
  float: left;
  margin-top: 48rpx;
  margin-left: 20rpx;
  width: 460rpx;
  height: 80rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #333333;
  line-height: 40rpx;
}
.default {
  margin: 32rpx 48rpx;
  height: 80rpx;
  overflow: hidden;
}
.default_l {
  float: left;
  height: 80rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #333333;
  line-height: 80rpx;
}
.default_r {
  float: right;
}
.button {
  position: fixed;
  bottom: 120rpx;
  left: 0rpx;
  width: 750rpx;
  height: 80rpx;
  background: #CBA55F;
  border-radius: 39rpx;
  text-align: center;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
  line-height: 80rpx;
}

