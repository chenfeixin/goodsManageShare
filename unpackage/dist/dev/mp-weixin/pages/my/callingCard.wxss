page {
  background: #F4F4F4;
}
.box {
  margin: 20rpx;
  width: 710rpx;
  height: 416rpx;
  background: linear-gradient(90deg, #31b8d6 0%, #27b3a6 100%);
  border-radius: 16rpx;
  overflow: hidden;
}
.box_name {
  margin-top: 44rpx;
  margin-left: 50rpx;
  width: 500rpx;
  height: 50rpx;
  font-size: 36rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
  line-height: 50rpx;
  overflow: hidden;
}
.box_name text {
  margin-left: 20rpx;
  color: #ffe200;
}
.box_c {
  margin-top: 24rpx;
  margin-left: 50rpx;
  height: 70rpx;
  overflow: hidden;
}
.box_c_box {
  float: left;
  width: 33%;
  height: 70rpx;
  overflow: hidden;
}
.box_c_box:nth-child(2) {
  width: 160rpx;
}
.box_c_box:nth-child(3) {
  width: 150rpx;
}
.box_c_box:nth-child(4) {
  width: 120rpx;
}
.box_c_box_t {
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
  line-height: 40rpx;
}
.box_c_box_f {
  margin-top: 2rpx;
  height: 28rpx;
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FFFFFF;
  line-height: 28rpx;
}
.box_f {
  margin-top: 24rpx;
  margin-left: 50rpx;
  height: 70rpx;
  overflow: hidden;
}
.box_f_t {
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
  line-height: 40rpx;
}
.box_f_t text {
  margin-left: 20rpx;
  color: #ffe200;
}
.box_f_b {
  margin-top: 2rpx;
  height: 28rpx;
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FFFFFF;
  line-height: 28rpx;
}
.box_time {
  margin-top: 24rpx;
  height: 70rpx;
  overflow: hidden;
}
.box_time .box_timebox {
  float: left;
  margin-left: 50rpx;
  width: 288rpx;
  height: 70rpx;
  overflow: hidden;
}
.box_time .box_timebox_t {
  width: 288rpx;
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
  line-height: 40rpx;
}
.box_time .box_timebox_b {
  margin-top: 2rpx;
  height: 28rpx;
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FFFFFF;
  line-height: 28rpx;
}

