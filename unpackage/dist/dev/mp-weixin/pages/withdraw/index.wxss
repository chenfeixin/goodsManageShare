  color: #333333;
  line-height: 80rpx;
}
.tool_boxes .tool_box_r {
  float: right;
  height: 80rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
