page {
  background: #F9F9F9;
}
.top {
  position: relative;
  margin: 36rpx 32rpx;
  width: 686rpx;
  height: 296rpx;
  border-radius: 16rpx;
  background: linear-gradient(135deg, #EDC66E 0%, #F6E1B0 100%);
  overflow: hidden;
}
.top_img {
  width: 686rpx;
  height: 296rpx;
}
.top_t {
  position: absolute;
  top: 48rpx;
  width: 686rpx;
  height: 60rpx;
  overflow: hidden;
}
.top_t_item {
  float: left;
  width: 25%;
  height: 60rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #000000;
  line-height: 44rpx;
  text-align: center;
}
.top_c {
  position: absolute;
  top: 130rpx;
  width: 686rpx;
  height: 34rpx;
  overflow: hidden;
}
.top_c_item {
  float: left;
  width: 25%;
  height: 34rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #404040;
  line-height: 34rpx;
  text-align: center;
}
.top_b {
  position: absolute;
  top: 200rpx;
  left: 207rpx;
  width: 686rpx;
  height: 80rpx;
  overflow: hidden;
}
.top_b_l {
  width: 262rpx;
  height: 80rpx;
  background: #EEC873;
  border-radius: 39rpx;
  text-align: center;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #000000;
  line-height: 80rpx;
}
.top_b_r {
  float: right;
  width: 272rpx;
  height: 72rpx;
  border-radius: 36rpx;
  border: 2rpx solid #FFFFFF;
  text-align: center;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FFFFFF;
  line-height: 72rpx;
  box-sizing: border-box;
}
.tool {
  margin: 36rpx 24rpx;
  overflow: hidden;
}
.tool_title {
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #333333;
  line-height: 40rpx;
}
.tool_title text {
  float: left;
  margin-top: 6rpx;
  margin-right: 12rpx;
  width: 8rpx;
  height: 28rpx;
  background: #EEC873;
  border-radius: 4rpx;
}
.tool_boxes {
  padding-top: 12rpx;
  padding-bottom: 60rpx;
  margin-top: 32rpx;
  width: 702rpx;
  background: #FFFFFF;
  border-radius: 16rpx;
}
.tool_boxes .tool_box {
  padding: 0rpx 24rpx;
  height: 80rpx;
  border-bottom: 2rpx solid #F9F9F9;
  overflow: hidden;
}
.tool_boxes .tool_box_l {
  float: left;
  height: 80rrpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
